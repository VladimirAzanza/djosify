container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python manage.py migrate"
  02_collectstatic:
    command: "source /var/app/venv/*/bin/activate && python manage.py collectstatic --noinput"
  03_run_gunicorn:
    command: "source /var/app/venv/*/bin/activate && gunicorn --workers 3 --threads 15 --bind 0.0.0.0:8000 djosify.wsgi:application"

